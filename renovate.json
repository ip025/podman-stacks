{
  "extends": [
    "config:recommended"
  ],
  "enabledManagers": [
    "docker-compose"
  ],
  "assignees": [
    "ip025"
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker-compose"],
      "matchPackageNames": ["docker.io/library/mariadb"],
      "allowedVersions": "<= 11.0.0",
      "description": "Keep MariaDB on version 11.x"
    }
  ]
}
